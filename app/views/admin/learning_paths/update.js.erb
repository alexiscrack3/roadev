<% if @learning_path.errors.any? %>
    $("#learning-path-errors").html("<%= j render("shared/error_messages", object: @learning_path) %>")
<% else %>
    $("#learning-path-<%= @learning_path.id %>").replaceWith("<%= j render("learning_path_row", learning_path: @learning_path) %>");
    myModal.toggle();
<% end %>
